<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.taranoate.MainActivity">


    <ViewFlipper
        android:id="@+id/viewFlipper_contact"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/picture10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/picture10" />

        <ImageView
            android:id="@+id/picture11"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/picture11" />


        <ImageView
            android:id="@+id/picture12"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/picture12" />

        <ImageView
            android:id="@+id/picture13"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/picture13" />


        <ImageView
            android:id="@+id/picture14"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/picture14" />


    </ViewFlipper>

    <ImageView
        android:id="@+id/tara_no_ate11"
        android:layout_width="279dp"
        android:layout_height="205dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tara_no_ate11" />

<android.support.constraint.ConstraintLayout
    android:layout_width="340dp"
    android:layout_height="260dp"
    android:layout_marginEnd="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="32dp"
    android:alpha="0.80"
    android:background="#ffffff"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/tara_no_ate11">

    <ImageView
        android:id="@+id/icon_pin"
        android:layout_width="40dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/icon_pin"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icon_hour"
        android:layout_width="40dp"
        android:layout_height="44dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_pin"
        app:srcCompat="@drawable/icon_hours" />



    <ImageView
        android:id="@+id/icon_phone"
        android:layout_width="40dp"
        android:layout_height="44dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_hour"
        app:srcCompat="@drawable/phone_phone" />

    <ImageView
        android:id="@+id/icon_email"
        android:layout_width="40dp"
        android:layout_height="44dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_phone"
        app:srcCompat="@drawable/icon_email" />

    <TextView
        android:id="@+id/address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="28dp"
        android:text="@string/contact2"
        android:textColor="#000000"
        android:onClick="address"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintLeft_toRightOf="@+id/icon_pin"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/hours"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/contact3"
        android:textColor="#000000"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/icon_hour"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.056"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/address" />

    <TextView
        android:id="@+id/telephone"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/contact4"
        android:onClick="telephone"
        android:textColor="#000000"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/icon_phone"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.056"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/hours" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/contact5"
        android:textColor="#000000"
        android:onClick="mail"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/icon_email"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.056"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/telephone"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="28dp" />

</android.support.constraint.ConstraintLayout>



</android.support.constraint.ConstraintLayout>